t.\")\n","            else:\n","                print('The mean of the column',\n","                      item, 'is: ', self.dataFrame[item].mean())\n","# p2 Q3\n","    def state_query(self, states):\n","        for item in states:\n","            print(self.dataFrame.loc[self.dataFrame['state'] == item]) # print that row if a value of 'state' column matches\n","\n","\n","class car_accidents(incident):\n","    def col_query(self, cols, states): # print rows and given columns of values matching within that column(state)\n","        self.dataFrame = self.dataFrame[cols]\n","        self.state_query(states)\n","\n","    def alcohol_states(self, level): # print states with alcohol level > given level\n","        print(self.dataFrame[self.dataFrame['alcohol'] > level])\n","\n","\n","ccdf = sns.load_dataset('car_crashes')\n","ccdf.rename(columns={'abbrev': 'state'}, inplace=True)\n","print('part 1 Q3')\n","print(ccdf)\n","\n","\n","# part 2\n","\n","testing = incident(ccdf, 'accidents', 'US')\n","print('\\npart 2 Q2')\n","testing.col_means(['speeding', 'alcohol', 'state']) # print column mean if the first(personal logic) value of the column is integer\n","print('\\npart 2 Q3')\n","testing.state_query(['CA', 'NY', 'TX']) # print that row if a value of 'state' column matches\n","\n","\n","# part 3\n","testing2 = car_accidents(ccdf, 'accidents', 'US')\n","print('\\npart 3 Q2')\n","testing2.col_query(['speeding', 'alcohol', 'state'], ['CA', 'NY', 'TX']) # print rows and given columns of values matching within that column(state)\n","print('\\npart 3 Q3')\n","testing2.alcohol_states(5) # print states with alcohol level > given level\n"]}]}